{
  "name": "status-sync",
  "version": "0.0.0",
  "description": "This repository contains a GitHub Actions workflow that lets you set and sync your real-time status on Slack, GitHub, and other places.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnandChowdhary/status-sync.git"
  },
  "keywords": [
    "status",
    "github",
    "slack",
    "life",
    "life-data",
    "github actions",
    "typescript"
  ],
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnandChowdhary/status-sync/issues"
  },
  "homepage": "https://github.com/AnandChowdhary/status-sync#readme",
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "automock": false
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^4.0.0",
    "@slack/web-api": "^6.0.0",
    "github-profile-status": "^1.0.0",
    "node-emoji": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node-emoji": "^1.8.1",
    "@vercel/ncc": "^0.27.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
